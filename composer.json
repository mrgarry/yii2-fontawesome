{
    "name": "modernkernel/yii2-fontawesome",
    "description": "Font Awesome extension for Yii2",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/modernkernel/yii2-fontawesome/issues?state=open",
        "source": "https://github.com/modernkernel/yii2-fontawesome"
    },
    "authors": [
        {
            "name": "Harry Tang",
            "email": "harry@modernkernel.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "bower-asset/font-awesome":"*"
    },
    "autoload": {
        "psr-4": {
            "modernkernel\\fontawesome\\": ""
        }
    }
}
